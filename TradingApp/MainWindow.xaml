<Window x:Class="TradingApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FinamBot" Height="400" Width="700">
    <Grid Margin="10">
        <Button Name="SendButton" Content="Получить деньги"
                HorizontalAlignment="Center" VerticalAlignment="Top"
                Padding="10,5" Click="SendButton_Click" Margin="0,133,0,0"/>
        <Button Content="Получить стакан SBER"
                Click="ShowOrderBookCommand_Click"
                Command="{Binding ShowOrderBookCommand}"
                Margin="265,182,265,152" Height="30" Width="150"/>
        <Button Content="Включить подписку на стакан SBER"
                Click="StartOrderBook_Click"
                Command="{Binding ShowOrderBookCommand}"
                Margin="237,231,237,103" Height="30"/>
    </Grid>
</Window>
